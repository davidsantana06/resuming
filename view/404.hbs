<html lang='en' data-theme='dark'>
  <head>
    <meta charset='utf-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1' />

    <title>Resuming</title>
    <link rel='icon' href='/static/favicon.ico' />

    <script
      src='https://kit.fontawesome.com/f3579f44cb.js'
      crossorigin='anonymous'
    ></script>

    <link rel='stylesheet' href='/static/vendor/bulma.min.css' />
    <link rel='stylesheet' href='/static/css/style.css' />
  </head>

  <body>
    <section class='hero is-fullheight'>
      <div
        class='hero-body is-flex is-flex-direction-column is-justify-content-center'
      >
        <div>
          {{! ~ logo ~ }}
          <figure class='image is-32x32 mb-3'>
            <img
              class='is-rounded'
              src='/static/img/_logo.png'
              alt='Resuming'
            />
          </figure>
          {{! ~ !}}

          {{! ~ title ~ !}}
          <h1 class='title is-5 is-uppercase mb-2'>404 Not Found</h1>
          {{! ~ !}}

          {{! ~ swagger-ui link ~ !}}
          <span>
            Lost?
            <a class='has-text-weight-medium' href='/swagger/ui'>
              <span class='icon-text'>
                <span>Access Swagger UI</span>
                <span class='icon'>
                  <i class='fas fa-arrow-up-right-from-square'></i>
                </span>
              </span>
            </a>
          </span>
          {{! ~ !}}
        </div>
      </div>
    </section>
  </body>

  <script src='/static/js/script.js'></script>
</html>